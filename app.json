{
  "expo": {
    "name": "xoma-app",
    "slug": "xoma-app",
    "version": "1.0.0",
    "orientation": "portrait",
    "icon": "./assets/images/old/icon.png",
    "scheme": "xomaapp",
    "userInterfaceStyle": "automatic",
    "newArchEnabled": true,
    "ios": {
      "supportsTablet": true
    },
    "android": {
      "adaptiveIcon": {
        "backgroundColor": "#E6F4FE",
        "foregroundImage": "./assets/images/old/android-icon-foreground.png",
        "backgroundImage": "./assets/images/old/android-icon-background.png",
        "monochromeImage": "./assets/images/old/android-icon-monochrome.png"
      },
      "edgeToEdgeEnabled": true,
      "predictiveBackGestureEnabled": false
    },
    "web": {
      "output": "static",
      "favicon": "./assets/images/old/favicon.png",
      "bundler": "metro"
    },
    "plugins": [
      "expo-router",
      [
        "expo-splash-screen",
        {
          "image": "./assets/images/old/splash-icon.png",
          "imageWidth": 200,
          "resizeMode": "contain",
          "backgroundColor": "#ffffff",
          "dark": {
            "backgroundColor": "#000000"
          }
        }
      ],
      "expo-web-browser"
    ],
    "experiments": {
      "typedRoutes": true,
      "reactCompiler": true
    },
    "extra": {
      "apiUrlDev": "localhost:3000",
      "apiUrlProd": "https://api.midominio.com"
    }
  }
}
